#include <iostream>
#include <cmath>

using namespace std;

int main(){
    double n1, n2, n3, d1, x1, x2, x3, x4, eq1, eq2;

    cout << "Digite o valor de A: " << endl;
    cin >> n1;
    cout << "Digite o valor de B: " << endl;
    cin >> n2;
    cout << "Digite o valor de C: " << endl;
    cin >> n3;

    //cálculos
    d1 = (n1 * n1) - (4 * n1 * n3);

    x1 = (-(n2) + sqrt(d1)) / (2 * n1);
    x2 = (-(n2) - sqrt(d1)) / (2 * n1);

    x3 = n1 * (x1 * x1) + n2 * x1 + n3;
    x4 = n1 * (x2 * x2) + n2 * x1 + n3;

    //final
    cout << "O resultado desta equação é = +" << x3 << " ou -" << x4 << endl;

system("pause");

}   

