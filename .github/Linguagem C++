#include <iostream>
#include <cmath>

using namespace std;

int main(){
    double n1, n2, PI, n3, areabase, arealateral, areatotalemcm, LTinta, LtTinta, LtTinta1p, arealateral2, arealateral1, custoLt, cm_para_m, m_para_m2, LtTintanecess, CustoTotal, quant_tanq, quant_tanq1, areabase1;

    cout << "Digite quantos tanques cilíndricos de combustível deseja pintar: " << endl;
    cin >> n1;
    cout << "Digite a altura desses cilíndros em cm: " << endl;
    cin >> n2;
    cout << "Digite o valor do raio desses cilíndros em cm: " << endl;
    cin >> n3;

    PI = M_PI;

    quant_tanq = n1 * n2;
    quant_tanq1 = n1 * n3;

    areabase = quant_tanq1 * quant_tanq1;
    areabase1 = PI * areabase;

    arealateral = 2 * PI;
    arealateral1 = quant_tanq1 * quant_tanq;
    arealateral2 = arealateral * arealateral1;

    areatotalemcm = areabase1 + areabase1 + arealateral2;

    LtTinta1p = 3 * 5;

    cm_para_m = areatotalemcm / 100;
    m_para_m2 = sqrt(cm_para_m);

    LtTintanecess = m_para_m2 / LtTinta1p;
    CustoTotal = LtTintanecess * 50;

    cout << "A quantidade necessária de latas de tinta é de " << floor(LtTintanecess) << " Latas.\nO custo total para pintar " << n1 << " tanques de combustíveis é de R$" << floor(CustoTotal) << endl;
    system("pause");
}
