#include <iostream>

using namespace std;

int main(){
    float n1, n2, comissao, salario_final, per_sf_sb1, per_sf_sb, per_sf_sb2;
    string nome;

    cout << "Digite o nome do vendedor: ";
    cin >> nome;
    cout << "Digite o salário base mensal correspondente ao vendedor " << nome << ": R$ ";
    cin >> n1;
    cout << "Digite o valor total de vendas realizadas pelo vendedor " << nome << " no mês: R$ ";
    cin >> n2;
 
    comissao = n2 * 15 / 100;
    salario_final = comissao + n1;
    per_sf_sb = salario_final - n1;
    per_sf_sb1 = salario_final * 100;
    per_sf_sb2 = per_sf_sb1 / n1;
    
    cout << "O vendedor " << nome << " possui um salario final no valor de: R$ " << salario_final << " assim recebendo " << per_sf_sb2 << "% em relação ao seu salário base mensal!" << endl;
    system("pause");
}

