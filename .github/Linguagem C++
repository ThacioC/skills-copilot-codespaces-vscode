    float n1, n2, c_1, c_2, c_3;

    cout << "Digite o valor do seu salário atual: ";
    cin >> n1;
    cout << "Digite a porcentagem do reajuste de seu salário: ";
    cin >> n2;

    c_1 = n2 / 100;
    c_2 = n1 * c_1;
    c_3 = c_2 + n1;

    cout << "Esse será seu salário com reajuste incluso: R$" << c_3 << endl;
    system("pause");
}

