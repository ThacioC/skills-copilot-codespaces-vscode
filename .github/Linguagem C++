#include <iostream>

using namespace std;

int main(){
    float n1, n2, mult, soma;

    cout << "Digite o preço de custo do produto: R$ ";
    cin >>  n1;
    cout << "Digite a margem de lucro em porcentagem: ";
    cin >> n2;

    mult = n1 * n2 / 100;
    soma = n1 + mult;

    cout << "Preço de venda do produto com adição da margem de lucro: "<< soma << endl;
    system("pause");
}

