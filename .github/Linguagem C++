    cin >> n1;
    cout << "Digite sua 2ª nota: ";
    cin >> n2;
    cout << "Digite sua 3ª nota: ";
    cin >> n3;

    div_p = n1 + n2 + n3;
    div_p1 = 1 + 2 + 3;
    div_p2 = div_p / div_p1;

    cout << "Sua média final é " << div_p2 << endl;
    system("pause");
