#include <iostream>

using namespace std;

int main(){
    float nm, nv, nr, nc, nkml,soma, soma1, soma2, num_abastc;

    cout << "Qual o comprimento da pista em metros: ";
    cin >> nm;
    cout << "Qual o número total de voltas a serem percorridas no grande prêmio: ";
    cin >> nv;
    cout << "Qual o número de reabastecimentos desejados: ";
    cin >> nr;
    cout << "Qual o consumo de combustível do carro em Km/L: ";
    cin >> nkml;

    soma = nm * nv;
    soma1 = soma / 1000;
    soma2 = soma1 / nkml;
    num_abastc = soma2 / nr;

    cout << "O número mínimo de litros necessários para até o primeiro reabastecimento é de " << num_abastc << "L" << endl;
    system("pause");
}

