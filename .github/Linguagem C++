#include <iostream>

using namespace std;

int main(){
    float n1, n2, c_0, c_1, c_2, c_3;

    cout << "Digite suas horas trabalhadas: " << endl;
    cin >> n1;
    cout << "Digite o valor do salário mínimo: R$" << endl;
    cin >> n2;

    c_0 = n2 / 5;
    c_1 = n1 * c_0;
    c_2 = c_1 * 0.03;
    c_3 = c_1 - c_2;

    cout << "O salário a receber pelo funcionário é de R$"<< c_3 << endl;

    system("pause");
}

