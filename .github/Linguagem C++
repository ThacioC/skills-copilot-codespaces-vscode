int main(){
    float n1, c_1, c_2, c_3, c_4;

    cout << "Digite o custo de fábrica do veículo: R$";
    cin >> n1;

    c_1 = n1 * 0.45;
    c_2 = n1 + c_1;
    c_3 = c_2 * 0.28;
    c_4 = c_3 + c_2;
    
    cout << "Esse é o custo final do veículo que você deseja: R$" << c_4 << endl;
    system("pause");
}

