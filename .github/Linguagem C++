#include <iostream>
#include <cmath>

using namespace std;

int main(){
    float n1, n2, n3, n4, v1, c1, c2, c3;

    cout << "Digite o preço do litro do combustivel: R$" << endl;
    cin >> n1;
    cout << "Digite a kilometragem inicial: " << endl;
    cin >> n2;
    cout << "Digite a kilometragem final: " << endl;
    cin >> n3;
    cout << "Digite a média de consumo do veículo: " << endl;
    cin >> n4;
    cout << "Digite o valor total recebido pelos passageiros: R$" << endl;
    cin >> v1;

    c1 = (n3 - n2) / n4;
    c2 = v1 - (n1 * c1);

    cout << "O número de litros de combustível gasto é de: " << c1 << endl;
    cout << "lucro líquido (R$) do dia: R$" << c2 << endl;

    system("pause");
}

