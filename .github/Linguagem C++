#include <iostream>

using namespace std;

int main(){
    char nome[30];
    float b1, b2, b3, b4, medaritmetica, soma,somasemestral1, somasemestral2, mediaaritmetica_S1, mediaaritmetica_S2;

    cout << "Digite seu nome: ";
    cin >> nome;
    cout << "Digite a nota do seu primeiro bimestre: ";
    cin >> b1;
    cout << "Digite a nota do seu segundo bimestre: ";
    cin >> b2;
    cout << "Digite a nota do seu terceiro bimestre: ";
    cin >> b3;
    cout << "Digite a nota do seu quarto bimestre: ";
    cin >> b4;

    soma = b1 + b2 + b3 + b4;
    medaritmetica = soma / 4;
    somasemestral1 = b1 + b2;
    somasemestral2 = b3 + b4;
    mediaaritmetica_S1 = somasemestral1 / 2;
    mediaaritmetica_S2 = somasemestral2 / 2;



    cout << nome << " sua média anual é: " << medaritmetica << endl;
    cout << nome << " sua média do primeiro semestre é: " << mediaaritmetica_S1 << endl;
    cout << nome << " sua média do segundo semestre é: " << mediaaritmetica_S2 << endl;
    system("pause");
}

