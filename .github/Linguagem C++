#include <iostream>

using namespace std;

int main(){
    float n1, n3, n2, c_1, c_2, c_3, c_4, c_5;

    cout << "Digite o peso do saco de ração em Kg: " << endl;
    cin >> n1;
    cout << "Digite a quantidade de ração fornecida para cada cachorro em g: " << endl;
    cin >> n2;
    cout << "Digite a quantidade de cachorros você irá alimentar: " << endl;
    cin >> n3;

    c_1 = n1 * 1000;
    c_2 = n2 * n3;
    c_3 = c_2 * 5;
    c_4 = c_1 - c_3;
    c_5 = c_4 / 1000;

    cout << "Essa é a quantidade de ração em gramas que restará no saco: " << c_5 << "Kg" << endl;
    system("pause");
}

