#include <iostream>

using namespace std;

int main(){
    double n1, n2, n3, n4, c_0, c_1, c_2, c_3, c_4, c_5, c_6;
//entrada--inicio
    cout << "Digite o valor depositado em sua conta bancária: R$" << endl;
    cin >> n1;
    cout << "Digite o valor da 1ª operação: " << endl;
    cin >> n2;
    cout << "Digite o valor da 2ª operação: " << endl;
    cin >> n3;
//cauculos
    c_0 = n2 * 0.0038;
    c_1 = n2 + c_0;
    c_2 = n3 * 0.0038;
    c_3 = n3 + c_2;
//cauculos2
    c_4 = c_0 + c_2;
    c_5 = c_1 + c_3;
    c_6 = n1 - c_5;
//final
    cout << "Essa foi o valor de seus lançamentos: "<< n1 << endl;
    cout << "Foi descontado de sua conta o valor de R$" << c_4 <<","" devido a tarifação de 0,38% em relação a cada operação bancária de retirada. " << endl;
    cout << "Seu saldo atual é de R$" << c_6 << endl; 
    
    system("pause");
}

